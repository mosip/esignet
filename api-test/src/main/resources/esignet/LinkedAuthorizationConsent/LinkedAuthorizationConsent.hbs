{
    "requestTime": "{{requestTime}}",
    "request": {
        "linkedTransactionId": "{{linkedTransactionId}}",
        "permittedAuthorizeScopes": [
	      {{#each permittedAuthorizeScopes}}
	            "{{scope}}"
	          {{#unless @last}},{{/unless}}
	      	{{/each}}
