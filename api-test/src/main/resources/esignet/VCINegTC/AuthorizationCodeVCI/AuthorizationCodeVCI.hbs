{
    "encodedHash": "{{encodedHash}}",
    "requestTime": "{{requestTime}}",
    "request": {
        "transactionId": "{{transactionId}}",
        "acceptedClaims": [
	      {{#each acceptedClaims}}
	            "{{claim}}"
	          {{#unless @last}},{{/unless}}
	      	{{/each}}
        ],
        "permittedAuthorizeScopes": [
	      {{#each permittedAuthorizeScopes}}
	            "{{scope}}"
	          {{#unless @last}},{{/unless}}
	      	{{/each}}
    ]
    }
}